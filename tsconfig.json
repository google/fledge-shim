{
  "exclude": ["dist/"],
  "compilerOptions": {
    "importHelpers": true,
    "target": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "angularCompilerOptions": { "enableIvy": false }
}
